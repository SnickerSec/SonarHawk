{
  "all": true,
  "include": [
    "index.js",
    "cli.js"
  ],
  "exclude": [
    "test/**",
    "node_modules/**",
    "web/**",
    "coverage/**",
    "*.config.js"
  ],
  "reporter": [
    "html",
    "text",
    "lcov"
  ],
  "report-dir": "./coverage",
  "temp-dir": "./coverage/.nyc_output",
  "check-coverage": true,
  "lines": 60,
  "statements": 60,
  "functions": 60,
  "branches": 50,
  "watermarks": {
    "lines": [60, 80],
    "functions": [60, 80],
    "branches": [50, 75],
    "statements": [60, 80]
  }
}
